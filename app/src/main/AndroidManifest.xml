<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/background"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        tools:targetApi="31">
        <activity
            android:name=".Profile"
            android:exported="false"
            android:label="@string/title_activity_profile"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".DetailMakanan"
            android:exported="false"
            android:label="@string/title_activity_detail_makanan"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".DaftarFavorite"
            android:exported="false"
            android:label="@string/title_activity_daftar_favorite"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".DaftarMakananPage"
            android:exported="false"
            android:label="@string/title_activity_daftar_makanan_page"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".Register"
            android:exported="false"
            android:label="@string/title_activity_register"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.MyApplication">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>